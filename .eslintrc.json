{
    "extends": ["react-app", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y", "react"],
    "rules": {
        "indent": ["off", 4],
        "no-unused-vars": ["warn"],
        "react/jsx-uses-react": "warn",
        "react/jsx-uses-vars": "warn"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"]
            }
        }
    }
}
